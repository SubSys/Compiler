module Core.Basics.Arithmetic
    exposing
        ( plus
        , (+)
        , (*)
        , (^)
        , (/)
        , (//)
        )


import Helm.Compiler.Sudo.Native


(+) : Int -> Int -> Int
(+) left right =
    Helm.Compiler.Sudo.Native.add left right


(+) : Float -> Float -> Float
(+) left right =
    Helm.Compiler.Sudo.Native.add left right


(-) : Int -> Int -> Int
(-) left right =
    Helm.Compiler.Sudo.Native.sub left right


(-) : Float -> Float -> Float
(-) left right =
    Helm.Compiler.Sudo.Native.sub left right


(*) : Int -> Int -> Int
(*) left right =
    Helm.Compiler.Sudo.Native.mul left right


(*) : Float -> Float -> Float
(*) left right =
    Helm.Compiler.Sudo.Native.mul left right


(^) : Int -> Int -> Int
(^) left right =
    Helm.Compiler.Sudo.Native.exp left right


(^) : Float -> Float -> Float
(^) left right =
    Helm.Compiler.Sudo.Native.exp left right


(/) : Float -> Float -> Float
(/) left right =
    Helm.Compiler.Sudo.Native.floatDiv left right


(//) : Int -> Int -> Int
(//) left right =
    Helm.Compiler.Sudo.Native.intDiv left right


plus x y =
    x + y


